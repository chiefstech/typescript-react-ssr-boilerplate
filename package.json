{
  "name": "typescript-react-ssr-boilerplate",
  "version": "0.5.0",
  "license": "MIT",
  "author": "thomasrogerlux@outlook.fr",
  "scripts": {
    "build-dev": "NODE_ENV=development webpack --watch",
    "build": "NODE_ENV=production webpack",
    "start-dev": "NODE_ENV=development nodemon build/server.js",
    "start": "NODE_ENV=production node build/server.js",
    "precommit": "npx tsc --noEmit && lint-staged",
    "test": "rm .babelrc & NODE_ENV=test yarn jest --config=config/jest/jest.config.js",
    "test:watch": "rm .babelrc & NODE_ENV=test yarn jest --config=config/jest/jest.config.js --watch --json --outputFile=.jest-test-results.json || true",
    "test:debug": "rm .babelrc & yarn -n=--inspect jest --runInBand --config=config/jest/jest.config.js --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "lint:fix",
      "git add"
    ],
    "*.{json,scss,sass,css,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "_moduleAliases": {
    "@cypress": "cypress",
    "@client": "src/client",
    "@server": "src/server",
    "@common": "src/common"
  },
